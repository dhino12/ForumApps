{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "cypress/globals": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "react-hooks",
        "cypress"
    ],
    "rules": {
        "indent": "off",
        "linebreak-style": "off",
        "quotes": "off",
        "react/jsx-filename-extension": [0],
        "react/react-in-jsx-scope": [0],
        "react/button-has-type": "off",
        "import/extensions": "off",
        "no-alert": "off",
        "no-underscore-dangle": "off"
    }
}
